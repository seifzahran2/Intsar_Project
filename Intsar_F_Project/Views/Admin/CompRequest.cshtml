@{
    Layout = null;
}
@model Intsar_F_Project.Models.CompReg
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>تحديد الوظيفه</title>
    <link rel="stylesheet" href="~/css/styleA.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">  
</head>
<body> 
    <form method="post"  id="logoutForm" >
    <div class="new-container" >
            <div class="navigation">
            <ul>
                <li>
                    <a href="#">
                        <span class="icon"></span>
                        <span class="title"></span>
                    </a>
                </li>
                <li>
                    <a asp-action="Index" asp-controller="Home">
                        <span class="icon">
                            <i class="bi bi-house-door"></i>
                        </span>
                        <span class="title">الصفحه الرئيسيه</span>

                    </a>
                </li>

                <li>
                    <a asp-action="UserMangment" asp-controller="Admin">
                        <span class="icon"><i class="bi bi-kanban"></i></span>
                        <span class="title">اداره المستخدمين</span>
                    </a>
                </li>
                <li>
                    <a asp-action="AdminHome" asp-controller="Admin">
                        <span class="icon">
                            <i class="bi bi-check2-circle"></i>

                        </span>
                        <span class="title">صفحه الرسائل وقبول الطلبات</span>
                    </a>
                </li>
                <li>
                    <a asp-action="ResultHome" asp-controller="Admin">
                        <span class="icon"><i class="bi bi-file-earmark-person"></i></span>
                        <span class="title">النتائج</span>
                    </a>
                </li>
            </ul>
        </div>
            <div class="main">
                <div class="topbar">

                    <div class="toggle">
                        <i class="bi bi-list"></i>
                    </div>
            
                    <!-- userImg -->
                    <div class="user">
                        <img src="../HOME PAGE/IMG/downloadasdasdas.jpg">
                    </div>
                    
                </div>
                <div class="appAccept">
                    <h1>جدول قبول الطلبات</h1> 
                    <form>
                        <table>
                                <!--  start head of table -->
                            <tr>
                                                
                                                <th>الأسم بالكامل</th>
                                                <th>البريد الالكتروني</th>
                                                <th>الرقم القومي</th>
                                                <th>التخصص</th>
                                                <th>النوع</th>
                            </tr>
                            <!--  start head of table -->
                            <tr>
                                    <td>@Model.FullName</td>
                                    <td>@Model.Email</td>
                                    <td>@Model.NationalId</td>
                                    <td>@Model.project_type</td>
                                    <td>@Model.Gender</td>
                            </tr>
                            
                        </table>
                    </form>
                    <div class="btnstyle">
                        <!-- <button type="button" id="btnAccept" class="btn btn-outline-secondary">قبول</button>
                        <button type="button" id="btnRefuse" class="btn btn-outline-secondary">رفض</button> -->
                        <button  type="submit" id="btnAccept" class="btn btn-secondary " >قبول</button>
                        <a asp-action="AdminHome" asp-controller="Admin" type="button" id="btnRefuse" class="btn btn-secondary " >رفض</a>
                    </div>
                </div>
            </div>
        </div>
        </form>
      <!-- JS codes -->
      <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
      <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
      <script src="~/script/scriptA.js"></script>
</body>
</html>